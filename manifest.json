{
    "manifest_version": 2,
    "name": "Fifa generator week view plugin",
    "short_name": "Week view plugin",
    "description": "A plugin to Fifa generator website that provides a week view",
    "version": "1.1.1",
    "author": "Andr√© Matos @apdmatos",
    "content_scripts": [
    {
      "js": [
        "./node_modules/jquery/dist/jquery.min.js",
        "index.js",
        "./node_modules/zone.js/dist/zone.js",
        "./node_modules/reflect-metadata/Reflect.js",
        "dist/bundle.js"
      ],
      "matches": [
        "http://www.fifagenerator.com/tournament/*"
      ]
    }
  ],

  "permissions": ["activeTab"]
}
